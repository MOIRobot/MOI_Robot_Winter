<?xml version="1.0"?>
<launch>

  <node pkg="move_base" type="move_base" respawn="false" name="move_base" output="screen">
  	  	 <remap from="cmd_vel" to="/smooth_cmd_vel"/>
  	  	<rosparam file="$(find winter_navigation)/teb_planner_costmap/costmap_common_params.yaml" command="load" ns="global_costmap" />
  	 	<rosparam file="$(find winter_navigation)/teb_planner_costmap/costmap_common_params.yaml" command="load" ns="local_costmap" />
  		<rosparam file="$(find winter_navigation)/teb_planner_costmap/local_costmap_params.yaml" command="load" />
  		<rosparam file="$(find winter_navigation)/teb_planner_costmap/global_costmap_params.yaml" command="load" />
  		<rosparam file="$(find winter_navigation)/teb_planner_costmap/teb_local_planner_params.yaml" command="load" />

		<param name="base_global_planner" value="global_planner/GlobalPlanner" />
		<param name="planner_frequency" value="1.0" />
		<param name="planner_patience" value="5.0" />

		<param name="base_local_planner" value="teb_local_planner/TebLocalPlannerROS" />
		<param name="controller_frequency" value="5.0" />
		<param name="controller_patience" value="15.0" />
	</node>
  
</launch>
